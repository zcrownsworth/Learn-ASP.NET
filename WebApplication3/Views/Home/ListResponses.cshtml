@model IEnumerable<WebApplication3.Models.GuestResponse>
@{ Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <rneta narne="viewport" content="width=device-width" />
    <title>Responses</title>
</head>
<body>
    <h2>
        Here is the list of people attending the party
    </h2>
    <table>
        <thead>
            <tr>
                <th>Name</th>
                <th>Email</th>
                <th>Phone</th>
            </tr>
        </thead>
        <tbody>
            @foreach (WebApplication3.Models.GuestResponse r in Model)
            {
            <tr>
                <td>@r.Name</td>
                <td>@r.Email</td>
                <td>@r.Phone</td>
            </tr>
            }
        </tbody>
    </table>
</body>
</html>